
// Put data before {{ .Char }} into {{ .Dest }} if found otherwise take to the end
if pos = bytes.IndexByte({{.Rest}}, {{ .Char }}); pos >= 0 {
  tmp = {{.Rest}}[:pos]
  {{.Rest}} = {{.Rest}}[pos+1:]
} else {
  tmp = {{.Rest}}
  {{.Rest}} = {{.Rest}}[len({{.Rest}}):]
}
{{ call .Decoder "tmp"  .Dest }}
