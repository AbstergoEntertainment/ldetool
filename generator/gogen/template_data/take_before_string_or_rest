
// Put data before {{ .ConstValue }} into {{ .Dest }} if found otherwise take to the end
if pos = bytes.Index({{.Rest}}, {{ .ConstName }}); pos >= 0 {
  tmp = {{.Rest}}[:pos]
  {{.Rest}} = {{.Rest}}[pos+len({{.ConstName}}):]
} else {
  tmp = {{.Rest}}
  {{.Rest}} = {{.Rest}}[len({{.Rest}}):]
}
{{ call .Decoder "tmp"  .Dest }}
