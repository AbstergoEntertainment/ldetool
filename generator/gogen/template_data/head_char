
// Checks if the rest starts with {{.Char}} symbol and pass it
if len({{.Rest}}) > 0 && {{.Rest}}[0] == {{.Char}} {
   {{.Rest}} = {{.Rest}}[1:]
} else {
{{if .Namespace}}p.{{.Namespace}}.Valid = false; goto {{.ScopeLabel}}{{else}}return false, {{ if .Serious }}fmt.Errorf("The rest (\033[1m%s\033[0m) doesn't start with `\033[1m%s\033[0m`", string({{.Rest}}), {{.Char}}){{else}}nil{{end}};{{end}}
}