
// Looking for {{ .Char }} symbol from {{.Lower}} until {{.Upper}} symbols of the rest and then pass it
if len(p.rest) < {{.Upper}} {
{{if .Namespace}}p.{{.Namespace}}.Valid = false; goto {{.ScopeLabel}}{{else}}return false, {{ if.Serious }}fmt.Errorf("Requested lookup in {{.Lower}}..%d-th symbols of the rest while only %s left", {{.Upper}}-1, len(p.rest)){{else}}nil{{end}};{{end}}
}
pos = bytes.IndexByte(p.rest[{{.Lower}}:{{.Upper}}], {{ .Char }});
if pos >= 0 {
    p.rest = p.rest[pos + {{.Lower}} + 1:]
}